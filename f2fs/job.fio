[global]
name=zns-f2fs
directory=/mnt/f2fs
ioengine=io_uring 
fixedbufs=1
registerfiles=1
sqthread_poll=1
iodepth=32
rw=randwrite
bs=4K
ioscheduler=mq-deadline
overwrite=1
group_reporting
file_service_type=random:1
loops=4
size=90G
ramp_time=10s
write_bw_log=${DATA}/bw_log
